<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView01"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:nestedScrollingEnabled="true"
    android:background="#f4c45b">
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context="com.example.asus.cameraapp.ChooseActivity"
        android:background="#f4c45b">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton01"
            android:background="#d5ae58"
            android:src="@drawable/stk01"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="false"
            android:layout_marginBottom="3dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton02"
            android:background="#d5ae58"
            android:src="@drawable/stk02"
            android:layout_alignParentTop="false"
            android:layout_toRightOf="@+id/StickerButton01"
            android:layout_marginLeft="3dp"
            android:layout_marginBottom="3dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton03"
            android:layout_alignParentRight="false"
            android:layout_alignParentEnd="false"
            android:background="#d5ae58"
            android:src="@drawable/stk03"
            android:layout_toRightOf="@+id/StickerButton02"
            android:layout_marginLeft="3dp"
            android:layout_marginBottom="3dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton04"
            android:background="#d5ae58"
            android:src="@drawable/stk04"
            android:layout_alignParentStart="false"
            android:layout_toRightOf="@+id/StickerButton03"
            android:layout_marginLeft="3dp"
            android:layout_marginBottom="3dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton05"
            android:layout_centerHorizontal="true"
            android:background="#d5ae58"
            android:src="@drawable/stk05"
            android:layout_below="@+id/StickerButton01"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="3dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton06"
            android:background="#d5ae58"
            android:src="@drawable/stk06"
            android:layout_toRightOf="@+id/StickerButton05"
            android:layout_below="@+id/StickerButton02"
            android:layout_marginLeft="3dp"
            android:layout_marginBottom="3dp" />
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton07"
            android:background="#d5ae58"
            android:src="@drawable/stk07"
            android:layout_alignParentTop="false"
            android:layout_alignParentLeft="false"
            android:layout_alignParentStart="false"
            android:layout_below="@+id/StickerButton03"
            android:layout_toRightOf="@+id/StickerButton06"
            android:layout_marginLeft="3dp"
            android:layout_marginBottom="3dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton08"
            android:background="#d5ae58"
            android:src="@drawable/stk08"
            android:layout_toRightOf="@+id/StickerButton07"
            android:layout_below="@+id/StickerButton04"
            android:layout_marginLeft="3dp"
            android:layout_marginBottom="3dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton09"
            android:layout_alignParentRight="false"
            android:layout_alignParentEnd="false"
            android:background="#d5ae58"
            android:src="@drawable/stk09"
            android:layout_below="@+id/StickerButton05"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="3dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton10"
            android:background="#d5ae58"
            android:src="@drawable/stk10"
            android:layout_alignParentLeft="false"
            android:layout_alignParentStart="false"
            android:layout_below="@+id/StickerButton07"
            android:layout_toRightOf="@+id/StickerButton09"
            android:layout_marginLeft="3dp"
            android:layout_marginBottom="3dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton11"
            android:layout_centerHorizontal="true"
            android:background="#d5ae58"
            android:src="@drawable/stk11"
            android:layout_toRightOf="@+id/StickerButton10"
            android:layout_below="@+id/StickerButton07"
            android:layout_marginLeft="3dp"
            android:layout_marginBottom="3dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton12"
            android:background="#d5ae58"
            android:src="@drawable/stk12"
            android:layout_below="@+id/StickerButton08"
            android:layout_toRightOf="@+id/StickerButton11"
            android:layout_marginLeft="3dp"
            android:layout_marginBottom="3dp" />
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton13"
            android:background="#d5ae58"
            android:src="@drawable/stk13"
            android:layout_alignParentTop="false"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="false"
            android:layout_below="@+id/StickerButton09"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="0dp" />
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton14"
            android:background="#d5ae58"
            android:src="@drawable/stk14"
            android:layout_toRightOf="@+id/StickerButton13"
            android:layout_below="@+id/StickerButton10"
            android:layout_marginLeft="3dp"
            android:layout_marginBottom="3dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton15"
            android:background="#d5ae58"
            android:src="@drawable/stk15"
            android:layout_alignParentTop="false"
            android:layout_alignParentRight="false"
            android:layout_alignParentEnd="false"
            android:layout_toRightOf="@+id/StickerButton14"
            android:layout_below="@+id/StickerButton11"
            android:layout_marginLeft="3dp"
            android:layout_marginBottom="3dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton16"
            android:background="#d5ae58"
            android:src="@drawable/stk16"
            android:layout_below="@+id/StickerButton12"
            android:layout_alignParentLeft="false"
            android:layout_alignParentStart="false"
            android:layout_toRightOf="@+id/StickerButton15"
            android:layout_marginLeft="3dp"
            android:layout_marginBottom="3dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/StickerButton17"
            android:layout_centerHorizontal="true"
            android:background="#d5ae58"
            android:src="@drawable/stk17"
            android:layout_below="@+id/StickerButton13"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="0dp" />


    </RelativeLayout>
</ScrollView>

